# Server Configuration
NODE_ENV=production
PORT=5001

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key

# JWT
JWT_SECRET=your-strong-jwt-secret-key-change-this
JWT_EXPIRES_IN=7d

# Twilio (optional fallback)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# VAPI AI Voice
VAPI_API_KEY=your-vapi-api-key
VAPI_ASSISTANT_ID=your-vapi-assistant-id
VAPI_PHONE_ID=your-vapi-phone-id

# Client URL (update with your Railway frontend URL)
CLIENT_URL=https://your-client-app.up.railway.app
